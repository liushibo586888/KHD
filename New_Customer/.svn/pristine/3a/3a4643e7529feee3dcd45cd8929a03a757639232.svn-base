
page,
page,
#app {
	height: 100%;
}
view {
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding: 0;
	margin: 0;
}
scroll-view {
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
/* 隐藏滚动条 */
scroll-view ::-webkit-scrollbar {
	width: 0;
	height: 0;
	background-color: transparent;
}
button {
	border-radius: 0;
}


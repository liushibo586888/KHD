<view><view class="page-body"><view class="page-section page-section-gap"><map style="width:100vw;height:100vh;" latitude="{{latitude}}" longitude="{{longitude}}" show-location scale="12" markers="{{covers}}" data-event-opts="{{[['callouttap',[['callouttap',['$event']]]]]}}" bindcallouttap="__e"></map></view></view><view data-event-opts="{{[['tap',[['showModel',['$event']]]]]}}" hidden="{{!(calloutDetailsTF)}}" class="hideModel" bindtap="__e"></view><view hidden="{{!(calloutDetailsTF)}}" class="MapBottom"><view class="MapBottomL"><view class="calloutDetailsTitle">{{calloutDetailsTitle}}</view><view class="calloutDetailsAddress">{{calloutDetailsAddress}}</view><view data-event-opts="{{[['tap',[['CallPhone',['$0'],['calloutDetailsPhone']]]]]}}" class="calloutDetailsPhone" bindtap="__e">{{calloutDetailsPhone}}</view></view><view data-event-opts="{{[['tap',[['mapOpen',['$event']]]]]}}" class="MapBottomR" bindtap="__e"><image mode="widthFix" src="../../static/tabbarImg/locate.png"></image></view></view></view>
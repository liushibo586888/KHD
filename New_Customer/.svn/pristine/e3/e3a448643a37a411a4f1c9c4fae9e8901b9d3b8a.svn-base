<view class="data-v-dcf46636"><view class="box data-v-dcf46636"><view style="font-weight:bold;font-size:35rpx;" class="data-v-dcf46636">{{month+" 月 "+year}}</view><scroll-view scroll-x="true" class="data-v-dcf46636"><block wx:for="{{dayList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block class="data-v-dcf46636"><view data-event-opts="{{[['tap',[['timeSelectd',[index]]]]]}}" class="{{['dayTitle data-v-dcf46636',current==index?'select':'']}}" bindtap="__e"><view class="data-v-dcf46636">{{item.day}}</view><block wx:if="{{index==0}}"><view style="font-size:25rpx;" class="data-v-dcf46636">今天</view></block><block wx:else><view style="font-size:25rpx;" class="data-v-dcf46636">{{"星期"+item.week}}</view></block></view></block></block></scroll-view><view class="timeItem data-v-dcf46636"><block wx:for="{{timeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block class="data-v-dcf46636"><view data-event-opts="{{[['tap',[['tabClick',[index]]]]]}}" class="{{['timechild data-v-dcf46636',tabIndex==index?'tabselected':'']}}" bindtap="__e">{{item}}</view></block></block></view></view><view class="btn data-v-dcf46636"><button class="{{['data-v-dcf46636',tabIndex==-1?'':'notstop']}}" type="default" disabled="{{tabIndex==-1?true:false}}" data-event-opts="{{[['tap',[['submitBtn',['$event']]]]]}}" bindtap="__e">确定</button></view></view>